@model GetProductUpdateQueryResult
@{
    Layout = null;
}


<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<Div class="Container">
    <form method="post">
        <label>Id</label>
        <input type="text" class="form-control" asp-for="Id" />
        <br />
        <label>Ürün Adı</label>
        <input type="text" class="form-control" asp-for="Name" />
        <br />
        <label>Ürün Stok Sayısı</label>
        <input type="text" class="form-control" asp-for=Stock />
        <br />
        <label>Ürün Fiyatı</label>
        <input type="text" class="form-control" asp-for=Price />
        <br />
        <button class="btn btn-success">Ürün güncelle</button>
    </form>
</Div>